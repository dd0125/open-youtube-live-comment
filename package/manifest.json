{
  "manifest_version": 2,
  "name": "Youtube Live Comment Open Other Tab",
  "description": "Youtube Live のコメントを他タブで開きます",
  "version": "1.0",
  "icons": {
    "32": "icon_32.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "browser_action": {
    "default_icon": "icon_32.png",
    "default_popup": "popup.html"
  },

  "permissions": [
    "tabs"
  ]
}